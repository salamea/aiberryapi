# AIBerry API Configuration
# Copy this file to .env and fill in your values

# Application
ENVIRONMENT=dev
DEBUG=false
APP_NAME=aiberry-api

# Google Studio API
GOOGLE_API_KEY=your_google_studio_api_key_here
GOOGLE_MODEL=gemini-1.5-flash

# Redis Configuration (Vector DB)
REDIS_HOST=redis-service.aiberry.svc.cluster.local
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Redis Memory Configuration
REDIS_MEMORY_HOST=redis-service.aiberry.svc.cluster.local
REDIS_MEMORY_PORT=6379
REDIS_MEMORY_DB=1

# Embedding Service
EMBEDDING_SERVICE_URL=http://aiberry-embeddings.aiberry.svc.cluster.local:8001
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Security
ALLOWED_ORIGINS=http://fend.aisolution.com
WHITELISTED_IPS=170.45.23.4

# Monitoring
ENABLE_METRICS=true
LOG_LEVEL=INFO
